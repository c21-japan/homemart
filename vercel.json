{
  "crons": [
    {
      "path": "/api/cron/daily-tasks",
      "schedule": "0 9 * * *"
    }
  ],
  "functions": {
    "app/properties/page.tsx": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/properties",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=60, stale-while-revalidate"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    },
    {
      "source": "/properties/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=300, stale-while-revalidate"
        }
      ]
    }
  ]
}
