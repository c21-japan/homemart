{
  "crons": [
    {
      "path": "/api/cron/daily",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/manager",
      "schedule": "0 23 * * *"
    },
    {
      "path": "/api/cron/weekly-flyer-analysis",
      "schedule": "0 9 * * 1"
    }
  ],
  "functions": {
    "app/properties/page.tsx": {
      "maxDuration": 10
    },
    "app/api/part-time-attendance/realtime/route.ts": {
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/properties",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=60, stale-while-revalidate"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    },
    {
      "source": "/properties/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=300, stale-while-revalidate"
        }
      ]
    },
    {
      "source": "/part-time-attendance",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    },
    {
      "source": "/admin/part-time-attendance",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DISABLE_CLERK": "1",
    "NEXT_PUBLIC_DISABLE_CLERK": "1"
  }
}
